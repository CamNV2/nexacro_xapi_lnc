<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.nexacro_xapi.api.mapper.ProjectMapper" >
    <select id="getAll"
            resultType="com.example.nexacro_xapi.entity.employee.ProjectEntity">
        SELECT A.pj_id, A.pjnm, A.pj_owner, A.pj_start_dt, A.pj_end_dt, A.pj_resolve_dt, A.temp_id
        FROM tb_project A;
    </select>
</mapper>